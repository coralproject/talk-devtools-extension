{
  "manifest_version": 2,
  "name": "Talk Devtools",
  "description": "This extension allows the developers to interact and debug Talk",
  "version": "1.0",
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Talk Devtool",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "web_accessible_resources": ["pageScript.js"],
  "permissions": [ "notifications", "contextMenus", "tabs", "storage", "<all_urls>", "activeTab"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:;"
}